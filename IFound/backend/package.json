{
  "name": "ifound-backend",
  "version": "1.0.0",
  "description": "Backend API for I Found!! crowdsourced finding platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "node src/utils/migrate.js",
    "seed": "node src/utils/seed.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "postinstall": "node scripts/download-ai-models.js",
    "setup-ai": "node scripts/download-ai-models.js",
    "test:scrape": "node src/tests/scraper/run.js",
    "test:scrape:stats": "node src/tests/scraper/run.js --stats",
    "test:generate": "node src/tests/generators/run.js",
    "test:seed": "node src/tests/seed/testDataSeed.js",
    "test:seed:resume": "node src/tests/seed/testDataSeed.js --resume",
    "test:validate": "node src/tests/validation/matchValidation.js",
    "test:validate:verbose": "node src/tests/validation/matchValidation.js --verbose",
    "test:report": "node src/tests/validation/reportGenerator.js",
    "test:report:html": "node src/tests/validation/reportGenerator.js --html",
    "test:full": "npm run test:scrape && npm run test:generate && npm run test:seed && npm run test:validate && npm run test:report"
  },
  "keywords": [
    "crowdsourcing",
    "finding",
    "bounty",
    "missing persons",
    "lost items"
  ],
  "author": "I Found Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs-node": "^4.15.0",
    "@vladmandic/face-api": "^1.7.12",
    "@xenova/transformers": "^2.17.2",
    "aws-sdk": "^2.1505.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jimp": "^0.22.10",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "natural": "^6.10.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.11",
    "sequelize": "^6.35.0",
    "socket.io": "^4.6.1",
    "stripe": "^14.7.0",
    "tesseract.js": "^7.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "puppeteer": "^24.34.0",
    "sharp": "^0.34.5",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
